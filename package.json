    "@types/react-dom": "^17.0.3",
    "babel-jest": "^27.0.2",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
